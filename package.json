{
  "name": "dohack-react-native-starter",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "MY_ENVIRONMENT=dev expo start -c",
    "storybook": "sb-rn-get-stories && STORYBOOK_ENABLED='true' npm start",
    "start:dev-client": "MY_ENVIRONMENT=dev expo start --dev-client -c",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:fetch-config": "node lib/build-fetch-config.js",
    "build:dev:android": "eas build --profile development --platform android --local",
    "build:qa:android": "eas build --profile preview --platform android --local",
    "build:dev:ios": "eas build --profile development-simulator --platform ios --local",
    "build:prod:ios": "eas build --profile production --platform ios --local",
    "build:eas:dev:android": "eas build --profile development --platform android",
    "build:eas:qa:android": "eas build --profile preview --platform android",
    "build:eas:dev:ios": "eas build --profile development-simulator --platform ios",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher"
  },
  "dependencies": {
    "@types/react": "~18.2.14",
    "expo": "~49.0.6",
    "expo-constants": "~14.4.2",
    "expo-linking": "~5.0.2",
    "expo-router": "2.0.0",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-screens": "~3.22.0",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-async-storage/async-storage": "^1.19.1",
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-native-community/slider": "^4.4.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-ondevice-actions": "^6.5.6",
    "@storybook/addon-ondevice-controls": "^6.5.6",
    "@storybook/react-native": "^6.5.6",
    "babel-loader": "^8.3.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "react-dom": "^18.2.0",
    "react-native-safe-area-context": "^4.7.1"
  },
  "private": true
}
